{
  "name": "bizmate/valueobjects",
  "description": "A PHP library/collection of classes aimed to help developers using and undestanding immutable objects.Forked from ytake/valueobjects due to unresponve PRs and broken builds.",
  "keywords": ["vo", "value object", "architecture", "domain driven design"],
  "license": "MIT",
  "authors": [
    {
      "name": "NicolÃ² Pignatelli",
      "email": "pignatelli.nicolo@gmail.com"
    },
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    },
    {
      "name": "Diego Gullo",
      "email": "diego_gullo@bizmate.biz"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ramsey/uuid": "^4.0 || ^3.0",
    "moneyphp/money": "^4.2",
    "marc-mabe/php-enum": "^4.0 || ^3.0",
    "league/geotools": "^1.1",
    "laminas/laminas-validator": "^2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11 ",
    "php-coveralls/php-coveralls": "^2.1.0",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "~3.7",
    "sebastian/phpcpd": "*",
    "phploc/phploc": "*",
    "pdepend/pdepend" : "^2.2.4"
  },
  "autoload": {
    "psr-4": {
      "ValueObjects\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ValueObjects\\Tests\\": "tests/"
    }
  },
  "replace": {
    "ytake/valueobjects": "^4.0"
  },
  "scripts": {
    "ci": [
      "./vendor/bin/phpunit",
      "./vendor/bin/phpcpd src/",
      "./vendor/bin/phploc src/ --log-xml=tests/build/phploc.xml"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
